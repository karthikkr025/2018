package test_package;

//2 Dimensional Arrey
public class Array_Class3 {
	public static void main(String[] args) {
		String str[][] = new String[2][3]; 
		str[0][0] ="Mumbai";
		str[0][1] ="Kolkata";
		str[0][2] ="Pune";
		
		str[1][0] ="Bangalore";
		str[1][1] ="Chennai";
		str[1][2] ="Dehli";
		
		int rownum = str.length;
		int colnum =str[0].length;
		
		
		
	}

}
